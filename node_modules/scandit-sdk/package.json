  "scripts": {
    "build": "run-p --print-label --aggregate-output build:* copy-engine-sdk",
    "build:browser-esm": "CACHE_FOLDER=esm rollup -c rollup.config.js -f es -o build/browser/index.esm.js && prettier --write build/browser/index.esm.js && CACHE_FOLDER=esm NODE_ENV=production rollup -c rollup.config.js -f es -o build/browser/index.esm.min.js && run-s \"replace:* build/browser/index.esm.js build/browser/index.esm.min.js\"",
    "build:browser-umd": "CACHE_FOLDER=umd rollup -c rollup.config.js -f umd --name ScanditSDK -o build/browser/index.js && prettier --write build/browser/index.js && CACHE_FOLDER=umd NODE_ENV=production rollup -c rollup.config.js -f umd --name ScanditSDK -o build/browser/index.min.js && run-s \"replace:* build/browser/index.js build/browser/index.min.js\"",
    "build:main": "mkdir -p build/main/styles && cpx src/styles/* build/main/styles && tsc -p tsconfig.json && run-s \"replace:* build/main/lib/workers/engineWorker.js\"",
    "build:module": "mkdir -p build/module/styles && cpx src/styles/* build/module/styles && tsc -p tsconfig.module.json && run-s \"replace:* build/module/lib/workers/engineWorker.js\"",
    "clean": "run-p --print-label clean:*",
    "clean:browser": "rm -rf build/browser",
    "clean:main": "rm -rf build/main",
    "clean:module": "rm -rf build/module",
    "copy-engine-sdk": "cpx \"scandit-engine-sdk/*\" build && run-s \"prepend:engine-version build/scandit-engine-sdk.min.js\"",
    "coverage": "run-s test coverage:generate && open-cli coverage/lcov-report/index.html",
    "coverage:check": "nyc report && nyc check-coverage --lines 90 --functions 90 --branches 90",
    "coverage:generate": "nyc report --reporter=lcov",
    "describe": "npm-scripts-info",
    "docs": "run-s docs:html && open-cli docs/index.html",
    "docs:copy-special": "mkdir -p docs && echo \"<html><head><link rel=\\\"stylesheet\\\" href=\\\"assets/css/main.css\\\"><style>body{margin:20px}</style></head><body>\" > docs/CHANGELOG.html && marked CHANGELOG.md >> docs/CHANGELOG.html && echo \"</body></html>\" >> docs/CHANGELOG.html && \\mv -f docs/CHANGELOG.html docs/CHANGELOG.md && cpx LICENSE docs",
    "docs:html": "rm -rf docs && typedoc --options typedoc.json && run-s docs:copy-special",
    "e2e": "nightwatch --suiteRetries 1 --retries 1 -e chrome,firefox,safari",
    "e2e-serve": "http-server ./ -a 127.0.0.1 -p 2674 -s",
    "fix": "run-p --print-label --aggregate-output fix:*",
    "fix:code": "tslint --fix --project . && prettier --write --loglevel warn \"src/**/*.ts\" \"e2e/**/*.js\" \"config/**/*.{js,json}\"",
    "fix:style": "stylelint --fix --syntax scss src/**/*.scss",
    "info": "npm-scripts-info",
    "license": "echo-cli \"`< LICENSE_HEADER`\" > LICENSE && yarn licenses generate-disclaimer --production >> LICENSE && cat LICENSE_MANUAL >> LICENSE && run-s docs:copy-special",
    "lint": "run-p --print-label --aggregate-output lint:*",
    "lint:stylelint": "stylelint --syntax scss src/**/*.scss",
    "lint:tsc": "tsc -p tsconfig.json --noEmit",
    "lint:tslint": "tslint --project .",
    "prepend:engine-version": "replace --quiet --count \"^\" \"wasmJSVersion=\\\"$(node -p \"require('./package.json').version\")\\\";\"",
    "release": "run-s reset clean license lint build test-no-build coverage:check docs:html && yarn standard-version",
    "replace:engine-version": "replace --quiet --count \"%VER%\" \"$(node -p \"require('./package.json').version\")\"",
    "replace:engine-wasm-hash": "replace --quiet --count \"%________________________ENGINE_WASM_HASH________________________%\" \"$(shasum -b -a 256 scandit-engine-sdk/scandit-engine-sdk.wasm | cut -d' ' -f1)\"",
    "reset": "git clean -dfx && git reset --hard && yarn",
    "test": "run-s build:main copy-engine-sdk && run-s \"replace:* build/main/lib/workers/engineWorker.js\" test-no-build",
    "test-no-build": "nyc --silent ava"
  },
  "scripts-info": {
    "info": "Display information about the scripts",
    "clean": "Remove build and test output folders",
    "build": "(Remove and re)build the library",
    "fix": "Try to automatically fix any linting problems",
    "lint": "Lint all source files",
    "test": "(Remove and re)build the main library and run unit tests",
    "e2e": "Run E2E tests",
    "coverage": "Run tests, generate the HTML coverage report, and open it in a browser",
    "docs": "Generate HTML API documentation and open it in a browser",
    "docs:json": "Generate API documentation in typedoc JSON format",
